generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model produto {
  id          Int      @id @default(autoincrement())
  name        String
  price       Float
  description String
  category_id Int
  stock       Int
  category category @relation(fields: [category_id], references: [id])
}

model users {
  id    Int    @id @default(autoincrement())
  name  String
  age   Int
  email String @unique
  password String
}

model category {
  id   Int    @id @default(autoincrement())
  name String
}